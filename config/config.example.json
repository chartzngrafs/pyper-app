{
    "navidrome": {
        "server_url": "http://localhost:4533",
        "username": "your_username",
        "password": "your_password",
        "database_path": "/var/lib/navidrome/navidrome.db",
        "ssh_host": "your.server.com",
        "ssh_user": "your_ssh_user",
        "ssh_key_path": "~/.ssh/id_rsa"
    },
    "ui": {
        "theme": "dark_teal",
        "window_width": 1400,
        "window_height": 900
    },
    "dynamic_themes": {
        "advanced_analysis": {
            "enabled": false,
            "audio_analysis": true,
            "sample_duration": 45,
            "analysis_timeout": 300,
            "parallel_workers": 2
        },
        "clustering": {
            "algorithm": "auto",
            "user_choice": true,
            "min_clusters": 5,
            "max_clusters": 25
        },
        "cache": {
            "max_size_mb": 100,
            "clear_on_analysis": true,
            "feature_cache_days": 30,
            "audio_features_cache": true
        },
        "external_services": {
            "musicbrainz_enabled": true,
            "lastfm_fallback": true,
            "rate_limit_delay": 1.2,
            "request_timeout": 10
        },
        "audio_features": {
            "bpm_analysis": true,
            "energy_analysis": true,
            "quantization_detection": true,
            "mood_detection": true,
            "spectral_analysis": true
        }
    },
    "_theme_options": {
        "qt_material_themes": [
            "dark_teal",
            "dark_purple", 
            "dark_amber",
            "dark_blue",
            "light_teal",
            "light_blue"
        ],
        "custom_themes": [
            "cobalt_blue",
            "ibm_patina_yellow",
            "hacker_green",
            "dracula",
            "tokyo_midnight",
            "monochrome",
            "synthwave84"
        ]
    }
} 